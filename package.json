{
  "name": "any-algorithm",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ivanoskov.github.io/any-algorithm",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.17",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.61",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "gh-pages": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "save-json-api": "node ./src/server/scripts/save-json-api.js",
    "build": "react-scripts build && npm run save-json-api && shx cp build/index.html build/404.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "json-server --watch ./src/server/db/index.js --routes ./src/server/routes.json --port 3001",
    "predeploy": "rm -rf build && npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "json-server": "^0.17.4",
    "node-fs": "^0.1.7",
    "shx": "^0.3.4"
  }
}
